    "class Solution(object):\n",
    "    def isPowerOfFour(self, num):\n",
    "        \"\"\"\n",
    "        :type num: int\n",
    "        :rtype: bool\n",
    "        \"\"\"\n",
    "        if num > 0 and num & (num - 1) == 0 and num & 0xaaaaaaaa == 0:\n",
    "            return True\n",
    "        return False"
