   "source": [
    "public class Solution {\n",
    "    public int FindComplement(int num) {\n",
    "        int todo = num;\n",
    "        int bit = 1;\n",
    "        while(todo != 0){\n",
    "            num = num ^ bit;\n",
    "            bit <<= 1;\n",
    "            todo >>= 1;\n",
    "        }\n",
    "        return num;\n",
    "    }\n",
    "}"
   ]
