   "source": [
    "class Solution(object):\n",
    "    def reverseStr(self, s, k):\n",
    "        \"\"\"\n",
    "        :type s: str\n",
    "        :type k: int\n",
    "        :rtype: str\n",
    "        \"\"\"\n",
    "        res = list(s)\n",
    "        for i in range(0, len(s), 2*k):\n",
    "            res[i:i + k] = reversed(res[i: i+k])\n",
    "        return ''.join(res)"
   ]
