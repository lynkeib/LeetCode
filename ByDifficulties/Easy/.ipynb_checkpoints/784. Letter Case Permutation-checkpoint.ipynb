    "class Solution(object):\n",
    "    def letterCasePermutation(self, S):\n",
    "        \"\"\"\n",
    "        :type S: str\n",
    "        :rtype: List[str]\n",
    "        \"\"\"\n",
    "        res = [[i.lower(), i.upper()] if i.isalpha() else i for i in S ]\n",
    "        return [''.join(i) for i in itertools.product(*res)]"
   ]
