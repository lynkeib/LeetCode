    "class Solution(object):\n",
    "    def hasAlternatingBits(self, n):\n",
    "        \"\"\"\n",
    "        :type n: int\n",
    "        :rtype: bool\n",
    "        \"\"\"\n",
    "        return all((i[0] != i[1] for i in zip(bin(n)[2:], bin(n)[2:][1:])))"
   ]
