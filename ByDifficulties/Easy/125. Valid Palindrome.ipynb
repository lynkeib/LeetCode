    "class Solution(object):\n",
    "    def isPalindrome(self, s):\n",
    "        \"\"\"\n",
    "        :type s: str\n",
    "        :rtype: bool\n",
    "        \"\"\"\n",
    "        return ''.join([i for i in s.lower() if i.isalnum()]) == ''.join([i for i in s.lower() if i.isalnum()])[::-1]"
   ]
