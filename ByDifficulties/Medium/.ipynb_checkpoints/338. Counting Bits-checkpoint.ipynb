    "class Solution(object):\n",
    "    def countBits(self, num):\n",
    "        \"\"\"\n",
    "        :type num: int\n",
    "        :rtype: List[int]\n",
    "        \"\"\"\n",
    "        res = [0]\n",
    "        while len(res) <= num:\n",
    "            res += [i+1 for i in res]\n",
    "        return res[:num+1]"
   ]
